<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FLIR Tools - Login Page tests">
    <!--    <listeners>-->
    <!--        <listener class-name="utils.AnnotationTransformer"/>-->
    <!--    </listeners>-->

    <parameter name="browserName" value="chrome"></parameter>
    <!--    Webrella env list: DEV,DEV-STG,FEATURE,FEATURE-STG,PROD,PROD-STG-->
    <parameter name="webrellaEnv" value="DEV"></parameter>
    <!--    SSO env list: LAB,PROD-->
    <parameter name="ssoEnv" value="LAB"></parameter>

    <test name="Login">
        <classes>
            <class name="testCases.LoginPageTest">
                <methods>
                    <include name="blankPassword_Test"></include>
                    <include name="loginWithNonExistingAccount_Test"></include>
                </methods>
            </class>
        </classes>
    </test>

</suite>