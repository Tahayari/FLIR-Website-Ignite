<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="IGNITE - Multiple flows Test Suite">
    <listeners>
        <listener class-name="utils.AnnotationTransformer"/>
    </listeners>

    <!--    Webrella env list: DEV,DEV-STG,FEATURE,FEATURE-STG,PROD,PROD-STG-->
    <parameter name="webrellaEnv" value="DEV"></parameter>
    <!--    SSO env list: LAB,PROD-->
    <parameter name="ssoEnv" value="LAB"></parameter>

    <test name="SmokeTest">
        <!--    browser list: chrome,edge,firefox-->
        <parameter name="browserName" value="firefox"></parameter>
        <classes>
            <class name="testCases.LoginPageTest">
                <methods>
                    <include name="blankEmail_Test"></include>
                </methods>
            </class>
        </classes>
    </test>

<!--    <test name="LoginPage">-->
